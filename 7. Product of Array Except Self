class Solution {
    public int[] productExceptSelf(int[] nums) {
        int prodR=1;
        int prodL=1;
        int num[]=new int[nums.length];
        for(int i=nums.length-1;i>=0;i--){
            num[i]=prodR;
            prodR=prodR*nums[i];
        }
        for(int i=0;i<nums.length;i++){
            num[i]=prodL*num[i];
            prodL=prodL*nums[i];
        }
        return num;
    }
}  
